<Window x:Class="WpfApp1.DeletePlayerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="DeletePlayerWindow" Height="450" Width="528">
    <Window.Background>
        <ImageBrush ImageSource="../Images/2.jpg" Stretch="UniformToFill" TileMode="None" />
    </Window.Background>
    <Grid>
        <ScrollViewer Margin="20,60,20,60">
            <WrapPanel Name="PlayerPanel" Background="LightGray" />
        </ScrollViewer>
        <Button  Content="Delete" HorizontalAlignment="Left" Margin="210,374,0,0" VerticalAlignment="Top" Width="91" Height="25" Click="Delete_Click" />
        <Label Content="Виберіть гравців" HorizontalAlignment="Left" Margin="184,23,0,0" VerticalAlignment="Top" Width="141" FontSize="16" FontFamily="Microsoft YaHei UI Light" Foreground="White"/>

    </Grid>
</Window>
